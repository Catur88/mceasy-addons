<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for mc_kontrak.mc_kontrak -->
        <record id="seq_mckontrak" model="ir.sequence">
            <field name="name">Contract</field>
            <field name="code">mc_kontrak.mc_kontrak</field>
            <field name="prefix">CT-%(day)s%(month)s%(year)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for sale.order-->
        <record id="seq_sale_order" model="ir.sequence">
            <field name="name">Sales Order</field>
            <field name="code">sale.order</field>
            <field name="prefix">SO-%(day)s%(month)s%(year)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for mc_kontrak.work_order-->
        <record id="seq_work_order" model="ir.sequence">
            <field name="name">Work Order</field>
            <field name="code">mc_kontrak.work_order</field>
            <field name="prefix">WO-%(day)s%(month)s%(year)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for mc_kontrak.churn_order-->
        <record id="seq_churn_order" model="ir.sequence">
            <field name="name">Churn Order</field>
            <field name="code">mc_kontrak.churn_order</field>
            <field name="prefix">CHURN-%(day)s%(month)s%(year)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>